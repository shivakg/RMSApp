@model IEnumerable<Rmsapp.ViewModels.CmsDataSummary>
<br />

@{
    ViewBag.Title = "CmsDataReport";
}

<br />

<h6>CmsDataReport</h6>


<table class="table table-striped table-condensed">
    <tr>
        <th>
            Pay Date
        </th>
        <th>
            Voucher Number
        </th>
        <th>
            Cheque Number
        </th>
         <th>
            Customer Number
        </th>
         <th>
            Customer Type
        </th>
        <th>
            Account Number
        </th>
        <th>
            Account Name
        </th>
        <th>
            Pay Mode
        </th>
        <th>
            Pay Amount
        </th>
        <th>
            Duplicate Count
        </th>
    </tr>

@foreach (var item in Model) {
    <tr>
     <td>
          
           @item.PayDate
        </td>
         <td>
         @item.VoucherNumber
        </td>
         <td>
          @item.ChequeNumber
        </td>
         <td>
         @item.CustomerNumber
        </td>
        <td>
            @item.CustomerType
        </td>
         <td>
         @item.AccountNumber
        </td>
         <td>
         @item.AccountName
        </td>
        <td>
         @item.PayMode
        </td>
        <td>
         @item.PayAmount
        </td>
         <td>
            @item.DupCount
        </td>
    </tr>
}
</table>
